@model ELearn.Models.Submission_Student

@{
    ViewData["Title"] = "Disqualify Submission";
}

<h3>Are you sure you want to disqualify this submission?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SubID)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SubID)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.StudentID)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.StudentID)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FileLink)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FileLink)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Grade)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Grade)
        </dd>
    </dl>
    
    <form asp-action="SubStuDelete">
        <input type="hidden" asp-for="SubStuID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <button type="button" class="btn btn-secondary" onclick="location.href='@Url.Action("SubIndex", "Lecturer")'">Back</button>
    </form>
</div>
